<% show_to :administrator do %>
  <div id="actions">
    <% link_to reaggregate_contest_url(), :method => "post", :title => t("views.recalc"), :id => "reaggregateContestAction" do %>
      <%= image_tag('actions/refresh.png') %><%= t ("actions.add") %>
    <% end %>
  </div>
<% end %>

<h1>
  <%= @contest.name %>
</h1>

<% tabnav :contest do %>
  <%= render :partial => "matchdays/standings", :locals => { :matchday => @matchday } %>
<% end %>
