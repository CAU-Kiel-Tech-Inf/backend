<%
render_tabnav :school, :html => { :class => "tabs_tabnav" } do
#  add_tab do |t|
 #   t.named  
  #end

  add_tab do |t|
    t.named t("titles.schools.details")
    begin; t.disable!; next; end if @school.new_record?
    t.links_to season_school_url(@season, @school)
    t.highlights_on :controller => "schools", :action => "show" 
  end unless @school.new_record?

  add_tab do |t|
    t.named t("actions.create").capitalize
    t.links_to new_season_school_url(@season)
    t.highlights_on :controller => "schools", :action => "new"
    t.highlights_on :controller => "schools", :action => "create"
  end if @school.new_record?

  add_tab do |t|
    t.named t("views.new_teams")
    t.links_to new_season_school_preliminary_contestant_url(@season, @school)
    t.highlights_on :controller => "preliminary_contestants", :action => "new"
    t.highlights_on :controller => "preliminary_contestants", :action => "create"
  end unless @school.new_record?

  add_tab do |t|
    t.named SurveyToken.human_name(:count => 2)
    t.links_to surveys_season_school_url(@season,@school)
    t.highlights_on :controller => "schools", :action => "surveys"
  end if @season.surveys_visible? and @school and not @school.new_record?

end
%>
