<h1>Editing contestant</h1>

<% form_for(@contestant) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :contest_id %><br />
    <%= f.text_field :contest_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @contestant %> |
<%= link_to 'Back', contestants_path %>