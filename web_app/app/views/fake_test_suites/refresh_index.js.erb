<% @fake_test_suites.each do |ft| %>
    $("#fake_test_suite_<%= ft.id %>  td.state").replaceWith("<td class='state'><%= (image_for_state(ft.state).gsub "\"", "'").html_safe! %></td>")
    <% if ft.started? and not ft.done? %>
     $("#restart_<%= ft.id %>").hide()
     $("#delete_<%= ft.id %>").hide()
    <% else %>
     $("#restart_<%= ft.id %>").show()
     $("#delete_<%= ft.id %>").show()
    <% end %>
<% end %>
