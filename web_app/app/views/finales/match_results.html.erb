<% dayname = @contest.finale.day_settings(@match.matchday.name.to_sym)[:human_name] %>
<h1>
  <%= link_to @contest.name, contest_url(@contest) %> /
  <%= link_to Finale.human_name, contest_finale_url(@contest) %> /
  <%= dayname %> /
  <%= @match.contestants.first.name + " vs " + @match.contestants.second.name %>
</h1>

<%= render :partial => "matches/match", :locals => {:match => @match, :allowQualify => false} %>

<% tooltip_init %>
