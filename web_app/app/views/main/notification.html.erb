<h1>
  <%= title %>
</h1>

<% tabnav tab do %>

<center>
<div class="notification">
  <div>
    <%= message.html_safe! %>
  </div>
  <div>
    <% links.each do |link| %>
      <div style="margin-top: 10px">
        <%= link_to link[0].html_safe!, link[1].html_safe! %>
      </div>
    <% end %>
  </div>
</div>
</center>

<% end %>
