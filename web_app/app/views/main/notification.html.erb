<h1>
  <%= title %>
</h1>

<% tabnav tab do %>

<center>
<div class="notification">
  <div>
    <%= message %>
  </div>
  <div>
    <% links.each do |link| %>
      <div style="margin-top: 10px">
        <%= link_to link[0], link[1] %>
      </div>
    <% end %>
  </div>
</div>
</center>

<% end %>
