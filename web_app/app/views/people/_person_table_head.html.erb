      <tr>
        <% if not logged_in? -%>
          <% attributes = %w{first_name last_name} %>
        <% else -%>
        <% if @contestant -%>
          <% attributes = %w{first_name last_name email} %>
        <% else -%>
          <% attributes = %w{first_name last_name email teams} %>
        <% end -%>
        <% end -%>

        <% attributes.each do |attr| %>
          <th><%= Person.human_attribute_name attr %></th>
        <% end %>
        <% if not @contestant %>
          <th>Admin</th>
        <% end %>
        <!-- options -->
        <th></th>
        <th></th>
        <th></th>
      </tr>
